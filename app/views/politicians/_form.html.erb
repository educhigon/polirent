
<%= simple_form_for(politician) do |f| %>
  <%= f.input :name %>
  <%= f.input :location %>
  <%= f.input :photo, as: :file %>
  <%= f.input :description %>
  <%= f.input :cost %>
  <%= f.input :tags, collection: %w(left right center sexy corrupt) %>
  <%= f.submit class: "btn btn-secondary" %>
<% end %>
